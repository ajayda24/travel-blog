{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npx tailwindcss -i ./style.css -o ./tailwind.css -w\" \"next dev \"",
    "build": "concurrently \"npx tailwindcss -i ./style.css -o ./tailwind.css --minify\" \"next build\"",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-tailwind/react": "^0.3.4",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/styles": "^5.1.0",
    "mongoose": "^6.0.12",
    "next": "11.1.2",
    "next-pwa": "^5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "babel-preset-next": "^1.4.0",
    "concurrently": "^6.3.0",
    "daisyui": "^1.16.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "postcss": "^8.3.9",
    "tailwindcss": "^2.2.17"
  }
